{
  "name": "vue-tel-input",
  "version": "9.7.1",
  "description": "International Telephone Input with Vue",
  "author": "Steven Dao <iamstevendao@gmail.com>",
  "license": "MIT",
  "homepage": "https://iamstevendao.com/vue-tel-input/",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamstevendao/vue-tel-input.git"
  },
  "exports": {
    ".": {
      "types": "./dist/vue-tel-input.d.ts",
      "import": "./dist/vue-tel-input.js",
      "require": "./dist/vue-tel-input.umd.cjs"
    },
    "./vue-tel-input.css": "./dist/vue-tel-input.css"
  },
  "main": "./dist/vue-tel-input.umd.cjs",
  "module": "./dist/vue-tel-input.js",
  "types": "./dist/vue-tel-input.d.ts",
  "scripts": {
    "dev": "vitepress dev docs",
    "test": "vitest --ui",
    "build": "vue-tsc && vite build",
    "postbuild": "cp src/types/index.d.ts dist/vue-tel-input.d.ts",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "libphonenumber-js": "^1.12.12",
    "vue": "^3.5.4"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitepress": "^1.0.0-rc.31",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  },
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "vue-tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone"
  ]
}
