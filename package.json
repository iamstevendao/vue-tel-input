{
  "name": "vue-tel-input",
  "version": "9.3.0",
  "description": "International Telephone Input with Vue",
  "author": "Steven Dao <iamstevendao@gmail.com>",
  "license": "MIT",
  "homepage": "https://iamstevendao.com/vue-tel-input/",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamstevendao/vue-tel-input.git"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-tel-input.js",
      "require": "./dist/vue-tel-input.umd.cjs",
      "types": "./dist/vue-tel-input.d.ts"
    },
    "./vue-tel-input.css": "./dist/vue-tel-input.css"
  },
  "main": "./dist/vue-tel-input.umd.cjs",
  "module": "./dist/vue-tel-input.js",
  "types": "./dist/vue-tel-input.d.ts",
  "scripts": {
    "dev": "cd ./docs && pnpm docs:dev",
    "test": "vitest --ui",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.app.json -outFile dist/vue-tel-input.d.ts",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "libphonenumber-js": ">=1.10",
    "vue": ">=3.5"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.1",
    "libphonenumber-js": "^1.11.17",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "vue-tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone"
  ],
  "pnpm": {
    "overrides": {
      "typescript": "5.7.2"
    }
  }
}